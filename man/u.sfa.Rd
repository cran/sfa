\name{u.sfa}
\alias{u.sfa}
\title{
Infficiencies of a sfa-object
}
\description{
returns the absolute inefficiencies of a sfa-object.
}
\usage{
u.sfa(object, ...)
}
\arguments{
  \item{object}{
an object of class sfa
}
  \item{\dots}{
ignored
}
}
\value{
returns the absolute inefficiencies of each observation
}
\seealso{
\code{\link{eff.sfa}}, \code{\link{te.eff.sfa}}
}
